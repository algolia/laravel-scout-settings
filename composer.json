{
    "name": "algolia/laravel-scout-settings",
    "description": "Import/Export Algolia settings into your Laravel Scout project",
    "keywords": [
        "algolia",
        "laravel",
        "search"
    ],
    "type": "library",
    "minimum-stability": "stable",
    "require": {
        "laravel/scout": "^3.0 || ^4.0",
        "algolia/algoliasearch-client-php": "^1.25"
    },
    "autoload": {
        "psr-4": {
            "Algolia\\Settings\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Algolia\\Settings\\Tests\\": "tests"
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Julien Bourdeau",
            "email": "julien.bourdeau@algolia.com"
        }
    ],
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Algolia\\Settings\\ServiceProvider"
            ]
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5",
        "mikey179/vfsStream": "^1.6",
        "orchestra/testbench": "^3.4"
    }
}
